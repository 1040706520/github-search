{
  "name": "GithubSearch",
  "description": "github search helper",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "GithubSearch",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "http://images.yantinglife.com/index.html",
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}